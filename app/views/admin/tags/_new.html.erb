<div class="container posts1">
  <div class="row">
    <div class="col-md-12 my-3">
      <h2 class="text-center my-2">タグ作成</h2>
      <%= form_with model: tag, url: admin_tags_path do |tag| %>
        <div class="d-flex justify-content-center">
          <%= tag.text_field :name %>
          <%= tag.submit '追加', class: 'btn btn-primary ml-2' %>
        </div>
      <% end %>
    </div>
  </div>
</div>
